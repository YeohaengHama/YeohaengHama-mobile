{
  "name": "@cloudtype/cli",
  "version": "0.7.1",
  "description": "cloudtype command line interface",
  "author": "joje <joje@cloudtype.co.kr>",
  "repository": "cloudtype/cli",
  "homepage": "https://cloudtype.io",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "bin": {
    "ctype": "bin/ctype.js",
    "cty": "bin/ctype-dev.js"
  },
  "preferGlobal": true,
  "scripts": {
    "prepublishOnly": "npm run build",
    "dist": "rimraf lib && tsc",
    "watch": "tsc -w --preserveWatchOutput",
    "build": "rimraf lib && tsc && rimraf dist && ncc build lib/index.js -m -o dist --license LICENSE",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "devDependencies": {
    "@cloudtype/connector": "^0.7.0",
    "@types/node": "^12.12.7",
    "@vercel/ncc": "^0.33.1",
    "chalk": "^2.4.2",
    "fs-extra": "^7.0.1",
    "inquirer": "^7.3.3",
    "json5": "^2.1.3",
    "keypair": "^1.0.4",
    "node-fetch": "^2.6.0",
    "node-forge": "^0.10.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "table": "^6.7.5",
    "tiny-date-format": "^0.1.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.20.1",
    "typescript": "^3.8.3",
    "untildify": "^4.0.0",
    "yaml": "^1.3.2",
    "yargs": "^14.2.3"
  },
  "files": [
    "bin",
    "dist"
  ]
}
